<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Connexion</servlet-name>
        <servlet-class>Connexion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AjouterProduit</servlet-name>
        <servlet-class>AjouterProduit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>bienvenue</servlet-name>
        <servlet-class>bienvenue</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Catalogue</servlet-name>
        <servlet-class>Catalogue</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AjaxServletForPanier</servlet-name>
        <servlet-class>AjaxServletForPanier</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Connexion</servlet-name>
        <url-pattern>/Connexion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AjouterProduit</servlet-name>
        <url-pattern>/AjouterProduit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>bienvenue</servlet-name>
        <url-pattern>/bienvenue</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Catalogue</servlet-name>
        <url-pattern>/Catalogue</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AjaxServletForPanier</servlet-name>
        <url-pattern>/AjaxServletForPanier</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            10
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>connexion.jsp</welcome-file>
    </welcome-file-list>
</web-app>
